<div class="details-container">
    <div class="bs">
        <header class="navbar">
            <div class="nav-wrap-left">
                <a class="react-back" href="javascript:history.back()"><i class="text-icon icon-back"></i></a>
            </div>
            <h1 class="nav-header">{{cinemaData.nm}}</h1>
        </header>
        <div class="download-header">
                <div class="logo-img"></div>
                <div class="info-wrapper">
                    <div class="title">猫眼</div>
                    <div class="content">0 元看电影，天天领现金</div>
                </div>
                <div class="link-btn">
                    立即领取
                </div>
            </div>
            <div class="cinema-wrap">
                <div class="cinema-block">
                    <div class="cinema-info">
                        <div class="title">{{cinemaData.nm}}</div>
                        <div class="location">{{cinemaData.addr}}</div>
                        <div class="location-icon" data-bid="dp_wx_cinema_map"><img src="data:image/png;base64,"></div>
                    </div>
                    <div class="swiper-container">
                        <div class="post-bg" ></div>
                        <div class="post-bg-filter"></div>
                        <div class="swiper-wrapper">
                        {{each showData.movies}}
                          <div class="swiper-slide"><img class="movie" src="{{$value.img}}"></div>
                        {{/each}}
                        </div>
                    </div>
                    <div class="movie-info">
                        <div class="movie-title line-ellipsis">
                            <span class="title">{{showData.movies[0].nm}}</span>
                            <span class="grade">
                                <span> {{showData.movies[0].sc}}<span class="small"> 分</span></span>
                            </span>
                        </div>
                        <div class="movie-desc line-ellipsis">{{showData.movies[0].desc}}</div>
                    </div>
                </div>
            </div>
    
    
            <div id="showDays">
                <ul id="timeline" class="mb-line-b">
                    {{each time}}
                        <li data-day="2019-10-14" class="showDay">
                            {{$value.month}} 月 {{$value.day}} 日
                        </li>
                    {{/each}}
                </ul>
            </div>
            <div class="seat-wrap">
                <div class="discount-block">
                    <div class="vip-tips">
                        <a>
                            <div class="label">折扣卡</div>
                            <div class="label-text line-ellipsis">现在开卡，首单 2 张最高立减 6 元</div> 	
                            <div class="process">20 元起开卡</div>
                        </a>
                    </div>
                </div>
                <div class="seat-inner-wrap">
                    <div class="seat-list">
                        <div class="list-wrap">
                            {{each showData.movies[0].shows[0].plist}}
                            <div class="item-outer mb-line-b" data-bid="dp_wx_cinema_show_item">
                                
                                <div class="item box-flex">
                                    <div class="time-block">
                                        <div class="begin">{{$value.tm}}</div>
                                        <div class="end">22:42<span class="tui"> 散场</span></div>
                                        
                                    </div>
                                    <div class="info-block">
                                        <div class="lan">{{$value.lang}}  {{$value.tp}}</div>
                                        <div class="hall">{{$value.th}}</div>
                                    </div>
                                    <div class="price">
                                        <div class="sellPr"><span class="d">¥</span><span><span class="stonefont">34</span></span></div>
                                        <div class="vipPrice"><span class="icon">折扣卡</span><span class="num"> ¥38</span></div>
                                        <div class="extraDesc">折扣卡首单特惠</div>
                                    </div>
                                    <div class="button-block">
                                        <div class="button" data-bid="dp_wx_cinema_show_btn">购票</div>
                                        
                                        
                                        
                                    </div>
                                </div>
                                
                            </div>
                            {{/each}}
                        </div>
                    </div>
                    
                </div>
            </div>
    </div>
</div>